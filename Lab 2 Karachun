import matplotlib.pyplot as plt

file = open("DS2.txt")
txt = []
x = []
y = []

with file as fl:
    for line in fl:
            txt.append([int(i) for i in line.split()])

for i in range(len(txt)):
    for j in range(1):
        x.append(txt[i][0])
        y.append(960 - txt[i][1])
plt.scatter(x, y)
plt.show()
plt.close()
file.close()
